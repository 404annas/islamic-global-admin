{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/v1/auth/:path*",
      "destination": "https://islamic-global-backend.vercel.app/api/v1/auth/:path*"
    },
    {
      "source": "/api/v1/user/:path*",
      "destination": "https://islamic-global-backend.vercel.app/api/v1/user/:path*"
    },
    {
      "source": "/api/v1/admin/:path*",
      "destination": "https://islamic-global-backend.vercel.app/api/v1/admin/:path*"
    },
    {
      "source": "/api/v1/contact/:path*",
      "destination": "https://islamic-global-backend.vercel.app/api/v1/contact/:path*"
    },
    {
      "source": "/((?!api/v1).*)",
      "destination": "/index.html"
    }
  ]
}